[Unit]
Description=UDM CNI Installer
After=udm-tools-boot-hooks.service
Wants=udm-tools-boot-hooks.service
Documentation=https://udm-tools.prasadt.com/services

[Service]
Type=oneshot
ExecStart=udmctl install-cni-plugins
RemainAfterExit=true

[Install]
WantedBy=multi-user.target
WantedBy=udm-tools-container-hooks.service
WantedBy=udm-tools-cni-hooks.service
